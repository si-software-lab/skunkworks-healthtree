{
  "index_patterns": ["events-*"],
  "template": {
    "settings": { "index": { "number_of_shards": 1 }},
    "mappings": {
      "properties": {
        "ts": {"type":"date","format":"epoch_millis"},
        "event_id": {"type":"keyword"},
        "provider_id": {"type":"keyword"},
        "type": {"type":"keyword"},
        "license_status": {"type":"keyword"}
      }
    }
  },
  "priority": 1,
  "data_stream": {}
}
